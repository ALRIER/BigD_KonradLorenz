      "source": [
        "%%R\n",
        "data_list <- list(c(\"enero\",\"Febrero\",\"Marzo\"), #aquí creo las listas\n",
        "                  matrix(c(1,2,3,4,-1,9), nrow = 2),#asigno categirias a lo que hay dentro\n",
        "                  list(\"Rojo\",12.3))\n",
        "data_list"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "VzyvMcNkeT42",
        "outputId": "2d97fdf4-44cd-451d-967b-df073a81fdc3"
      },
      "execution_count": 36,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "[[1]]\n",
            "[1] \"enero\"   \"Febrero\" \"Marzo\"  \n",
            "\n",
            "[[2]]\n",
            "     [,1] [,2] [,3]\n",
            "[1,]    1    3   -1\n",
            "[2,]    2    4    9\n",
            "\n",
            "[[3]]\n",
            "[[3]][[1]]\n",
            "[1] \"Rojo\"\n",
            "\n",
            "[[3]][[2]]\n",
            "[1] 12.3\n",
            "\n",
            "\n"
          ]
        }
